
/* test path constraint generation */
machine simple{

    abstract start state S0{}
    abstract state S1{}
    abstract state S2{}

    transition t1 { S0 -> S1 }
    transition t2 { S1 -> S0 }
    transition t3 { S1 -> S2 }
    transition t4 { S2 -> S1 }

    /* goal section */
    goal{
        check for 2 reach (S1)
    }
}